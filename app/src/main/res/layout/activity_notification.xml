<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".raw.Menu_Activity.Notification_Activity">
    <androidx.appcompat.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#7FBFF6"
        android:id="@+id/toolbar_not"
        android:fitsSystemWindows="true"
        app:navigationIcon="@drawable/fh"
        >
        <TextView
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="通知设置"
            android:textColor="@color/white"
            android:textSize="22sp" />

    </androidx.appcompat.widget.Toolbar>
  <androidx.appcompat.widget.SwitchCompat
      android:layout_width="match_parent"
      android:layout_height="50dp"
      android:text="开启通知"
     android:textSize="20sp"
      android:paddingHorizontal="16dp"
      app:thumbTint="@color/white"
      app:trackTint="@color/switch_hk"
      >

  </androidx.appcompat.widget.SwitchCompat>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:paddingLeft="16dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:text="点击跳转权限设置"
        android:background="@drawable/sbw_ty"

        android:textColor="#000000"
        android:textSize="20sp">

    </TextView>
    <androidx.appcompat.widget.SwitchCompat
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="提示音"
        android:textSize="20sp"
        android:background="@drawable/sbw_ty"
        android:paddingHorizontal="16dp"
        app:thumbTint="@color/white"
        app:trackTint="@color/switch_hk"
        >
    </androidx.appcompat.widget.SwitchCompat>
    <androidx.appcompat.widget.SwitchCompat
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="震动"
        android:textSize="20sp"
        android:background="@drawable/sbw_ty"
        android:paddingHorizontal="16dp"
        app:thumbTint="@color/white"
        app:trackTint="@color/switch_hk"
        >
    </androidx.appcompat.widget.SwitchCompat>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/sbw_ty"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="16dp"
            android:paddingTop="10dp"
            android:background="@drawable/sbw_ty"
            android:paddingBottom="10dp"
            android:text="设定目标树脂"
            android:textColor="#000000"
            android:textSize="20sp">

        </TextView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:paddingLeft="16dp"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:text="当前：160"
            android:layout_marginRight="10dp"
            android:textColor="#000000"
            android:textSize="20sp">

        </TextView>
    </RelativeLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:text="正在施工目前不可用"
        android:textSize="25sp"
        android:textAlignment="center"
        >

    </TextView>

</LinearLayout>